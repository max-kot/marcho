$(".hero-slider").slick({dots:!0,infinite:!0,fade:!0,arrows:!1,autoplay:!0,speed:800,autoplaySpeed:2e3});const menuBtn=document.querySelector(".menu-btn"),menuList=document.querySelector(".menu__list"),body=document.querySelector("body"),menuLinks=document.querySelectorAll(".menu a"),titleBtn=(menuBtn.addEventListener("click",function(){body.classList.toggle("no-scroll"),menuList.classList.toggle("active"),menuBtn.classList.toggle("active")}),menuLinks.forEach(function(e){e.addEventListener("click",function(){menuList.classList.remove("active"),body.classList.remove("no-scroll"),menuBtn.classList.remove("active")})}),document.querySelectorAll(".footer-menu__title")),titleList=document.querySelectorAll(".footer-menu__list");titleBtn.forEach(function(e){e.addEventListener("click",function(){this.nextElementSibling.classList.toggle("active"),this.classList.toggle("active")})});